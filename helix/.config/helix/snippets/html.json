{
  "HTML5 Boilerplate": {
    "prefix": "!",
    "body": [
      "<!DOCTYPE html>",
      "<html lang=\"${1:en}\">",
      "<head>",
      "\t<meta charset=\"UTF-8\">",
      "\t<meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">",
      "\t<title>${2:Document}</title>",
      "</head>",
      "<body>",
      "\t$0",
      "</body>",
      "</html>"
    ],
    "description": "HTML5 boilerplate"
  },
  "div with class": {
    "prefix": ".",
    "body": "<div class=\"${1:class}\">$2</div>",
    "description": "div with class (Emmet style)"
  },
  "div with id": {
    "prefix": "#",
    "body": "<div id=\"${1:id}\">$2</div>",
    "description": "div with id (Emmet style)"
  },
  "element with class": {
    "prefix": "${TM_CURRENT_WORD}.",
    "body": "<${TM_CURRENT_WORD} class=\"${1:class}\">$2</${TM_CURRENT_WORD}>",
    "description": "any element with class"
  },
  "element with id": {
    "prefix": "${TM_CURRENT_WORD}#",
    "body": "<${TM_CURRENT_WORD} id=\"${1:id}\">$2</${TM_CURRENT_WORD}>",
    "description": "any element with id"
  },
  "link CSS": {
    "prefix": "link:css",
    "body": "<link rel=\"stylesheet\" href=\"${1:style}.css\">",
    "description": "Link CSS file"
  },
  "script source": {
    "prefix": "script:src",
    "body": "<script src=\"${1:script}.js\"></script>",
    "description": "External JavaScript file"
  },
  "image": {
    "prefix": "img",
    "body": "<img src=\"${1:source}\" alt=\"${2:alternative text}\">",
    "description": "Image element"
  },
  "anchor": {
    "prefix": "a",
    "body": "<a href=\"${1:#}\">${2:Link}</a>",
    "description": "Anchor link"
  },
  "unordered list": {
    "prefix": "ul",
    "body": [
      "<ul>",
      "\t<li>${1:Item}</li>",
      "</ul>"
    ],
    "description": "Unordered list"
  },
  "ordered list": {
    "prefix": "ol",
    "body": [
      "<ol>",
      "\t<li>${1:Item}</li>",
      "</ol>"
    ],
    "description": "Ordered list"
  },
  "list item": {
    "prefix": "li",
    "body": "<li>${1:Item}</li>",
    "description": "List item"
  },
  "navigation": {
    "prefix": "nav",
    "body": [
      "<nav>",
      "\t<ul>",
      "\t\t<li><a href=\"${1:#}\">${2:Home}</a></li>",
      "\t</ul>",
      "</nav>"
    ],
    "description": "Navigation element"
  },
  "form": {
    "prefix": "form",
    "body": [
      "<form action=\"${1:/submit}\" method=\"${2:post}\">",
      "\t$0",
      "</form>"
    ],
    "description": "Form element"
  },
  "input text": {
    "prefix": "input:text",
    "body": "<input type=\"text\" name=\"${1:name}\" id=\"${1:name}\" placeholder=\"${2:placeholder}\">",
    "description": "Text input field"
  },
  "input email": {
    "prefix": "input:email",
    "body": "<input type=\"email\" name=\"${1:email}\" id=\"${1:email}\" placeholder=\"${2:email@example.com}\">",
    "description": "Email input field"
  },
  "input password": {
    "prefix": "input:password",
    "body": "<input type=\"password\" name=\"${1:password}\" id=\"${1:password}\">",
    "description": "Password input field"
  },
  "textarea": {
    "prefix": "textarea",
    "body": "<textarea name=\"${1:name}\" id=\"${1:name}\" rows=\"${2:4}\" cols=\"${3:50}\">${4:Content}</textarea>",
    "description": "Textarea element"
  },
  "button": {
    "prefix": "button",
    "body": "<button type=\"${1:button}\">${2:Button}</button>",
    "description": "Button element"
  },
  "table": {
    "prefix": "table",
    "body": [
      "<table>",
      "\t<thead>",
      "\t\t<tr>",
      "\t\t\t<th>${1:Header}</th>",
      "\t\t</tr>",
      "\t</thead>",
      "\t<tbody>",
      "\t\t<tr>",
      "\t\t\t<td>${2:Data}</td>",
      "\t\t</tr>",
      "\t</tbody>",
      "</table>"
    ],
    "description": "Complete table structure"
  },
  "table row": {
    "prefix": "tr",
    "body": [
      "<tr>",
      "\t<td>${1:Data}</td>",
      "</tr>"
    ],
    "description": "Table row"
  },
  "table header": {
    "prefix": "th",
    "body": "<th>${1:Header}</th>",
    "description": "Table header cell"
  },
  "table data": {
    "prefix": "td",
    "body": "<td>${1:Data}</td>",
    "description": "Table data cell"
  },
  "meta viewport": {
    "prefix": "meta:viewport",
    "body": "<meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">",
    "description": "Viewport meta tag"
  },
  "meta charset": {
    "prefix": "meta:charset",
    "body": "<meta charset=\"UTF-8\">",
    "description": "Character encoding meta tag"
  },
  "meta description": {
    "prefix": "meta:description",
    "body": "<meta name=\"description\" content=\"${1:Description}\">",
    "description": "Description meta tag"
  },
  "link to external site": {
    "prefix": "link:external",
    "body": "<link rel=\"${1:stylesheet}\" href=\"${2:https://example.com/style.css}\">",
    "description": "External resource link"
  },
  "header": {
    "prefix": "header",
    "body": [
      "<header>",
      "\t<h1>${1:Title}</h1>",
      "</header>"
    ],
    "description": "Header element"
  },
  "main": {
    "prefix": "main",
    "body": [
      "<main>",
      "\t$0",
      "</main>"
    ],
    "description": "Main content element"
  },
  "footer": {
    "prefix": "footer",
    "body": [
      "<footer>",
      "\t<p>${1:Â© 2024 Company}</p>",
      "</footer>"
    ],
    "description": "Footer element"
  },
  "section": {
    "prefix": "section",
    "body": [
      "<section>",
      "\t<h2>${1:Section Title}</h2>",
      "\t$0",
      "</section>"
    ],
    "description": "Section element"
  },
  "article": {
    "prefix": "article",
    "body": [
      "<article>",
      "\t<h2>${1:Article Title}</h2>",
      "\t$0",
      "</article>"
    ],
    "description": "Article element"
  },
  "aside": {
    "prefix": "aside",
    "body": [
      "<aside>",
      "\t<h3>${1:Sidebar}</h3>",
      "\t$0",
      "</aside>"
    ],
    "description": "Aside element"
  },
  "video": {
    "prefix": "video",
    "body": "<video src=\"${1:video.mp4}\" controls width=\"${2:640}\" height=\"${3:360}\"></video>",
    "description": "Video element"
  },
  "audio": {
    "prefix": "audio",
    "body": "<audio src=\"${1:audio.mp3}\" controls></audio>",
    "description": "Audio element"
  },
  "canvas": {
    "prefix": "canvas",
    "body": "<canvas id=\"${1:canvas}\" width=\"${2:640}\" height=\"${3:480}\"></canvas>",
    "description": "Canvas element"
  },
  "svg": {
    "prefix": "svg",
    "body": "<svg width=\"${1:100}\" height=\"${2:100}\">$0</svg>",
    "description": "SVG element"
  },
  "bootstrap container": {
    "prefix": "bs:container",
    "body": "<div class=\"container\">$0</div>",
    "description": "Bootstrap container"
  },
  "bootstrap row": {
    "prefix": "bs:row",
    "body": [
      "<div class=\"row\">",
      "\t<div class=\"col\">$1</div>",
      "</div>"
    ],
    "description": "Bootstrap row and column"
  },
  "tailwind container": {
    "prefix": "tw:container",
    "body": "<div class=\"container mx-auto px-4\">$0</div>",
    "description": "Tailwind CSS container"
  },
  "flexbox container": {
    "prefix": "flex",
    "body": "<div class=\"flex\">$0</div>",
    "description": "Flexbox container"
  },
  "grid container": {
    "prefix": "grid",
    "body": "<div class=\"grid\">$0</div>",
    "description": "Grid container"
  }
}